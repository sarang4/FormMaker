			Hidden Field
		    </label>
		</div>
		<div class="control-group">
		    <label class="control-label" for="select_options">Select Options</label>
		    <div class="well well-small clearfix">
			{{each(i,choice) options}}
				<div class="single_select_option">
				    <input type="text" style="margin-top:10px;" class="select_input" value="${choice.value}"/>
				    <span class="add_single_select_option" style="pointer:cursor"><i class="icon-plus-sign"></i></span>
				    <span class="delete_single_select_option" style="pointer:cursor"><i class="icon-minus-sign"></i></span>
				</div>
			{{/each}}
			<a class="btn btn-mini btn-info pull-right" id="select_options_done" style="margin-right:20px;">Done</a>
		    </div>
		</div>
	    </div>
    </div>
</script>


